import java.util.Scanner;
 

 
public class Java_estrutura {

    public static void main(String[] args) {
        int resposta = 20;
         if (resposta== 20){
      // se a variavel for igual a 20, a frase abaixo será escrita
          System.out.println("você acertou!");
    }
       // se a variavel não for igual a 20 nenhuma frase será exibida
    }
    } 
